<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Efficiency__c</fullName>
    <formula>IF(Experience__c &gt;= 1,
    Totel_Project_Assigned__c / 2,
    NULL
)</formula>
